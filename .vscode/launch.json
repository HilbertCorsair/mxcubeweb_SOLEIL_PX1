    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["--help"],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "GEVENT_SUPPORT": "True",
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python Debug",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": ["-r", "test/HardwareObjectsMockup.xml", "--static-folder", "/home/mateusz/Documents/MXCUBE/mxcube3-new/ui"],
            "subProcess": true,
            "env": {
                "GEVENT_SUPPORT": "True"
            }
        }
    ]
}