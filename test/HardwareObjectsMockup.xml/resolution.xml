  <channel type="tango" polling="500" name="minimum_distance">distanceMin</channel>
  <channel type="tango" polling="500" name="minimum_resolution">resolutionMin</channel>

  <!--<channel type="tango" polling="500" name="minimum_resolution">resolutionMinForCollect</channel>-->
