<?xml version="1.0" encoding="iso-8859-1"?>
<beamline_pars>
  <COLLECT_PARS>
    <fileinfo>
      <directory>/data/data1</directory>
      <prefix>user_</prefix>
      <run_number>1</run_number>
    </fileinfo>
    <current_filename></current_filename>
    <dark>1</dark>
  </COLLECT_PARS>
  <SPEC_PARS>
    <phi>phi</phi>
    <wavmot>lambda</wavmot>
    <emot>energy</emot>
    <detdistmot>dtox</detdistmot>
    <beam>
         <ax>-0.003579583</ax>
         <ay>-0.004106302</ay>
         <bx>2052.060</bx>
         <by>2232.887</by>
    </beam>
    <beamsize>
      <!-- determined by apertures -->
      <max_hor>0.080</max_hor>
      <max_ver>0.030</max_ver>
    </beamsize>

    <detector>
      <binning>1</binning>
      <save_raw>0</save_raw>
      <no_xform>0</no_xform>
      <DarkTime>1</DarkTime>
      <UseDefaultDarkTime>1</UseDefaultDarkTime>
    </detector>
    <maximum_counts>2</maximum_counts>
    <intensity_calibration_constant>1</intensity_calibration_constant>
  </SPEC_PARS>
  <BCM_PARS>
    <synchrotron_name>SOLEIL</synchrotron_name>
    <undulator>
        <type>u20</type>
        <device_uri>id/id/29</device_uri>
        <gap_index>0</gap_index>
    </undulator>
    <monochromator_type>channel cut</monochromator_type>
    <detector_radius>150.</detector_radius>
    <pixel_size_x>0.075</pixel_size_x>
    <pixel_size_y>0.075</pixel_size_y>
    <hutch>proxima1</hutch>
  </BCM_PARS>
 <VALIDATE_PARS>
    <exposure_time>0.008,100</exposure_time>
    <start_angle>-1000,1000</start_angle>
    <range>0.001,10</range>
    <number_of_passes>1,10</number_of_passes>
    <start_image_number>0,10000</start_image_number>
    <run_number>0,10000</run_number>
    <overlap>-90,0.99</overlap>
    <number_of_images>1,50000</number_of_images>
 </VALIDATE_PARS>
 <EXTRAVALIDATE_PARS>
    <rotation_speed>0.01,100</rotation_speed>
 </EXTRAVALIDATE_PARS>
 <INHOUSE_USERS>
   <proposal>
     <code>opid</code>
     <number>141</number>
   </proposal>
   <proposal>
     <code>mxihr</code>
   </proposal>
 </INHOUSE_USERS>

</beamline_pars>
